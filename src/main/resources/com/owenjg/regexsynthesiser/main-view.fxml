<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>
<?import javafx.geometry.Insets?>
<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="com.owenjg.regexsynthesiser.controller.MenuController" spacing="20" alignment="CENTER">
    <padding>
        <Insets top="20" bottom="20" left="20" right="20"/>
    </padding>

    <Label fx:id="titleText" text="A Regular Expression Synthesiser using positive and negative examples!"/>

    <Button text="Start" onAction="#onStartButtonClick"/>
    <Button text="Quit" onAction="#onQuitButtonClick"/>
</VBox>
